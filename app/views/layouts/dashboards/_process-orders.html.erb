<div class="text-center"> <h2>Orders</h2><br></div>
<table id="process-order-table" class="table table-striped table-bordered" style="width:100%">
        <thead>
            <tr>
                <th>Order Id</th>
                <th>User Id</th>
                <th>Restaurant Id</th>
                <th>Placed At</th>
                <th>Status</th>
                <th></th>
            </tr>
        </thead>

        <tbody>
            <% @neworders.each do |order|%>
                <tr id="process-row-<%=order.id%>">
                    <td><%= order.id %></td>
                    <td><%= order.user_id %></td>
                    <td><%= order.restaurant_id %></td>
                    <td><%= order.created_at %></td>
                    <td id="process-order-status-<%=order.id%>"><%= order.status %></td>
                    <td><%= link_to "Change Status","/orders/change_status/"+order.id.to_s, remote: true %>
                </tr>
            <% end %>
        </tbody>
</table>